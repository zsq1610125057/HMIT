<sqlMap>
<insert id="addInv" parameterClass="com.vos.Invoice">
<![CDATA[
insert into invoice values(#invId#,#invMoney#,#invContent#,#invType#,str_to_date(#invDate#,'%m/%d/%Y'),#invRemarks#,#proId#)
]]>
</insert>
</sqlMap>